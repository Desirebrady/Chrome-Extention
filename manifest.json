{
    "name": "Linkedin extension ",
    "description": "Linkdin extension",
    "version": "0.0.1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "./background.js"
        ]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "./images/16.png"
        }
    },
    "permissions": [
        "tabs",
        "activeTab",
        "notifications",
        "http://*/",
        "https://*/",
        "https://www.linkedin.com/mynetwork/"
    ]
}